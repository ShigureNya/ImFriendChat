<resources>
    <!-- Default screen margins, per the Android Design guidelines. -->
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <!--第一视图，状态栏和tabView的高度  主要按钮高度-->
    <dimen name="main_toolbar_height">?attr/actionBarSize</dimen>
    <dimen name="main_tabview_height">40dp</dimen>
    <dimen name="main_btn_height">?attr/actionBarSize</dimen>
    <!--FloatingActionButton的大小-->
    <dimen name="pyq_floating_button_size">60dp</dimen>
    <!--文字大小 名称为第一大小 消息为第二大小 时间为第三大小-->
    <dimen name="main_text_size">16dp</dimen>
    <dimen name="second_text_size">12dp</dimen>
    <dimen name="third_text_size">10dp</dimen>
    <!--标题字体大小与非主标题大小 非主标题即不包含在主界面的标题-->
    <dimen name="main_title_size">18dp</dimen>
    <dimen name="no_main_title_size">14sp</dimen>
    
    <dimen name="cardview_width_size">7dp</dimen>
    <dimen name="cardview_height_size">3dp</dimen>

    <dimen name="search_view_inset">10dp</dimen>
</resources>
